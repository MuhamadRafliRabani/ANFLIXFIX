

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model library {
  id            Int     @id @default(autoincrement())
  mal_id        Int
  email         String
  images        String
  title         String
  year          Int
  score         Float
  status        String
  @@unique([mal_id, email, images, title])
}